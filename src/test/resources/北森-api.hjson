{
 rules: [
    {
        searchPatterns: [
           {
             pattern: .name.\s*:\s*.(.+).,\s*.cent.:\s*.(.*).,
             nameIndex: 1
             valueIndex: 2
             nameMatchers: @notContains(胜任力基础)
             nameFilters:
                '''
                @unPrefix(MPF,FAST,MPv2,MS_M_)
                @unPostfix(V2,CAT)
                @map(通用管理技能测验,管理技能总分)
                @map(_自定义,综合得分)
                @map(_CATA,综合得分)
                '''
             valueFilters: @trimTrailingZeroes
             tempVarsMap:
                '''
                战略理解与执行 a1 目标设置 a2 规划安排 a3 时间管理 a4
                任务分配 b1 授权管理 b2 团队管理 b3
                决策判断 c1 激励推动 c2 培养下属 c3 沟通协调 c4 关系管理 c5
                监查反馈 d1 应变调控 d2 绩效管理 d3
                '''
           }
        ],
        patternTexts:[
           {
              pattern: .create_time.:\s*.(\S+\d)?.
              valueIndex: 1
              name: 测试完成时间
              valueFilters: @normalizeDateTime
           },
           {
              pattern: .activity_id.:\s*(\S+?),
              valueIndex: 1
              name: activityId
              temp: true
           }
        ]
    }
  ],
  evals: [
    {
      condition: activityId == '387249'
      expr: ((float)a1 + (float)a2 + (float)a3 + (float)a4) / 4
      name: 计划管理技能
    },
    {
      condition: activityId == '387249'
      expr: ((float)b1 + (float)b2 + (float)b3) / 3
      name: 组织管理技能
    },
    {
      condition: activityId == '387249'
      expr: ((float)c1 + (float)c2 + (float)c3 + (float)c4 + (float)c5) / 5
      name: 领导管理技能
    },
    {
      condition: activityId == '387249'
      expr: ((float)d1 + (float)d2 + (float)d3) / 3
      name: 控制管理技能
    },
  ]
}